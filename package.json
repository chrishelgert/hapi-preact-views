    "babel-register": "^6.26.0",
    "hapi": "^17.0.2",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "preact": "^8.2.6",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "vision": "^5.2.0"
